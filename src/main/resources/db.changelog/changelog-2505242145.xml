<?xml version="1.0" encoding="UTF-8" ?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">
    <changeSet  author="Abhinav Jain"  id="2505242145">
        <createTable tableName="candidate">
            <column  name="id"  type="INTEGER" autoIncrement="true">
                <constraints  nullable="false"  primaryKey="true"  unique="true"/>
            </column>
            <column name="registration_number" type="varchar(255)">
                <constraints nullable="false" unique="true"/>
            </column>
            <column name="name" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="marital_status" type="varchar(15)"/>
            <column name="dob" type="DATE"/>
            <column name="tob" type="TIME"/>
            <column name="place" type="varchar(255)"/>
            <column name="education" type="varchar(255)"/>
            <column name="occupation_type" type="varchar(15)"/>
            <column name="occupation_detail" type="varchar(255)"/>
            <column name="income" type="varchar(127)"/>
            <column name="height" type="varchar(5)"/>
            <column name="weight" type="INTEGER"/>
            <column name="complexion" type="varchar(31)"/>
            <column name="manglik" type="BOOLEAN"/>
            <column name="gotra" type="varchar(127)"/>
            <column name="nakshatra" type="varchar(127)"/>
            <column name="mama_gotra" type="varchar(127)"/>
            <column name="father_name" type="varchar(255)"/>
            <column name="mother_name" type="varchar(255)"/>
            <column name="parent_occupation_type" type="varchar(15)"/>
            <column name="parent_occupation_detail" type="varchar(255)"/>
            <column name="parent_income" type="varchar(127)"/>
            <column name="siblings" type="jsonb"/>
            <column name="address" type="varchar(255)"/>
            <column name="contact_number" type="varchar(10)"/>
            <column name="poc" type="varchar(255)"/>
            <column name="poc_contact_number" type="varchar(10)"/>
            <column name="special" type="varchar(127)"/>
            <column name="other_details" type="varchar(127)"/>
            <column name="photo_url" type="varchar(255)"/>
            <column name="created_at" type="TIMESTAMP"/>
            <column name="updated_at" type="TIMESTAMP"/>
        </createTable>
    </changeSet>
</databaseChangeLog>